<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegServlet</servlet-name>
  	<display-name>RegServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.RegServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogServlet</servlet-name>
  	<display-name>LogServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.LogServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>controllers.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegServlet</servlet-name>
  	<url-pattern>/RegServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogServlet</servlet-name>
  	<url-pattern>/LogServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
</web-app>
